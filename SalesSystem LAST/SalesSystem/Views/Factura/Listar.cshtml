
<table class="table" id="idFactura">
    <thead>
        <tr>
            <th>
                Codigo
            </th>
            <th>
                Nombre
            </th>
            <th>
                Precio
            </th>
            <th>
                IVA
            </th>
            <th>
                Cantidad
            </th>
            <th class="columnaASumar">
                Total
            </th>
        </tr>
    </thead>
    <tbody>
    </tbody>
</table>

<script>
    
    function sumarColumna() {
        var tabla = document.getElementById("idFactura");
        var filas = tabla.getElementsByTagName("tr");
        var suma = 0;

    for (var i = 1; i < filas.length; i++) {
        var celda = filas[i].getElementsByClassName("columnaASumar")[0];
        var valor = parseFloat(celda.textContent);
        suma += valor;
    }
        document.getElementById("labelCompra_Importe")getElementsByTagName = suma;
}
    </script>